                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/phd-common/tsconfig.lib.json",
                        "project": "projects/phd-common/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/phd-common/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/phd-common/src/test.ts",
                        "tsConfig": "projects/phd-common/tsconfig.spec.json",
                        "karmaConfig": "projects/phd-common/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
